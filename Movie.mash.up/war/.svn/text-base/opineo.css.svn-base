@charset "utf-8";
.red-bar{
  height:30px;
  -moz-border-radius: 5px;
  border-radius: 5px;
	
  -webkit-box-shadow: rgba(115, 115, 115, 1) 0px 1px 1px;
  -moz-box-shadow: rgba(115, 115, 115, 1) 0px 1px 0px; /* FF 3.5+ */
  
  text-align:center;
  margin:0px 5px 5px 0px;
  float:left;
  font-size:12px; font-weight:bold;
  padding-top:0px;

}


.red-bar p{
  font-size:12px; font-weight:bold;
  padding-top:8px;
  margin:0px;
}


