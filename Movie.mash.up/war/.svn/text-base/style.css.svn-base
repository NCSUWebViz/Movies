.pagination {
	text-align: center;
	background: url(images/arrow.png);
}
.pagination a {
	background: url(images/arrow.png) 0 -300px no-repeat transparent;
	width: 15px;
	height: 15px;
	margin: 0 5px 0 0;
	display: inline-block;
}
.pagination a.selected {
	background-position: -25px -300px;
	cursor: default;
}
.pagination a span {
	display: none;
}


