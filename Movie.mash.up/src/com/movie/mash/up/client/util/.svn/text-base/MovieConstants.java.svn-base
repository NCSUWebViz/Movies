	/**
	 * TODO : Can use Enum
	 * @param movieOrigin
	 * @return
	 */
	public static int getMovieOriginCode(String movieOrigin) {

		if (movieOrigin.equals(IMDB_MOVIE_ORIGIN)) {
			return IMDB_MOVIE_ORIGIN_CODE;
		} else if (movieOrigin.equals(ROTTEN_TOMATOES_MOVIE_ORIGIN)) {
			return ROTTEN_TOMATOES_MOVIE_ORIGIN_CODE;
		} else if (movieOrigin.equals(NETFLIX_MOVIE_ORIGIN)) {
			return NETFLIX_MOVIE_ORIGIN_CODE;
		} else if (movieOrigin.equals(NYTIMES_MOVIE_ORIGIN)) {
			return NYTIMES_MOVIE_ORIGIN_CODE;
		}
		
		return -1;
	}

